{
	"manifest_version": 3,
	"name": "DevTools Interceptor (v2)",
	"version": "2.0.0",
	"description": "Aba no DevTools para criar regras que modificam headers via declarativeNetRequest.",
	"permissions": ["declarativeNetRequest", "storage", "tabs"],
	"host_permissions": ["<all_urls>"],
	"background": {
		"service_worker": "service_worker.js"
	},
	"devtools_page": "startup.html",
	"action": {
		"default_title": "DevTools Request Interceptor"
	}
}
